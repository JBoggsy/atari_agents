sp {vop_graph_test*propose*step-1
   (state <s> ^name vop_graph_test
             -^step1 <s1-state>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name step-1)
}

sp {apply*step-1
   (state <s> ^operator <op>
              ^svs.command <cmd-link>)
   (<op> ^name step-1)
-->
   (<s> ^step1 complete)
   (<cmd-link> ^add-vop-node <add-vop-node-cmd>)
   (<add-vop-node-cmd> ^op-type get-from-vsm)
}
